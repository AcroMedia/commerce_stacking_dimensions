<?php

/**
 * @file
 * Install routine.
 */

/**
 * Implements hook_install().
 */
function commerce_stacking_dimensions_install() {
  field_create_field(array(
    'field_name' => 'stacking_dimensions',
    'type' => 'physical_dimensions',
    'cardinality' => FIELD_CARDINALITY_UNLIMITED,
    'module' => 'physical',
    'active' => 1,
  ));
}